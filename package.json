    "optics-agent": "^1.1.8",
    "passport": "^0.4.0",
    "passport-github": "^1.1.0",
    "persistgraphql": "^0.3.11",
    "pg": "^7.3.0",
    "pg-connection-string": "^2.0.0",
    "reddit-score": "^0.0.2",
    "request-promise": "^4.2.2",
    "subscriptions-transport-ws": "^0.9.1"
  }
}
