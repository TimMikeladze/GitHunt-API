{
  "name": "githunt-api",
  "version": "1.0.0",
  "description": "Example app for Apollo",
  "scripts": {
    "start": "babel-node api/index.js",
    "dev": "nodemon api/index.js --watch api --exec babel-node",
    "lint": "eslint api migrations seeds",
    "test": "jest && npm run lint",
    "test:watch": "jest --watch",
    "seed": "knex seed:run",
    "migrate": "knex migrate:latest",
    "test:setup": "rm test.sqlite3 || true; knex migrate:latest --env test && knex seed:run --env test"
  },
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/apollostack/GitHunt.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/apollostack/GitHunt/issues"
  },
  "homepage": "https://github.com/apollostack/GitHunt#readme",
  "devDependencies": {
    "babel-eslint": "^8.0.1",
    "babel-jest": "^21.2.0",
    "casual": "^1.5.17",
    "eslint": "^4.10.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "jest": "^21.2.1",
    "nodemon": "^1.12.1",
    "sqlite3": "^3.1.13"
  },
  "dependencies": {
    "apollo-engine": "https://s3.us-east-2.amazonaws.com/apollo-engine-deploy/apollo-engine-0.3.5.tgz",
    "apollo-server-express": "^1.2.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.26.0",
    "body-parser": "^1.18.2",
    "compression": "^1.7.1",
    "connect-session-knex": "^1.4.0",
    "cookie": "^0.3.1",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.4",
    "dataloader": "^1.3.0",
    "dotenv": "^4.0.0",
    "express": "^4.16.2",
    "express-session": "^1.15.6",
    "graphql": "^0.11.7",
    "graphql-server-express": "^1.2.0",
    "graphql-subscriptions": "^0.5.4",
